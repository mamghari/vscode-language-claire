{
    "name": "claire",
    "displayName": "language-claire-colorizer",
    "description": "Claire language support for vscode",
    "version": "0.0.1",
    "publisher": "mamghari",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "claire",
            "aliases": ["CLAIRE", "claire"],
            "extensions": [".cl"],
            "configuration": "./claire.configuration.json"
        }],
        "grammars": [{
            "language": "claire",
            "scopeName": "source.claire",
            "path": "./syntaxes/claire.json"
        }]
    }
}
